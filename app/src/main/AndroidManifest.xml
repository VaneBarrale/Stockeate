<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stockeate.stockeate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="Stockeate"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Stockeate">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity_Home"/>
        <activity android:name=".Activity_Registrar"/>
        <activity android:name=".Activity_Recuperar"/>

    </application>

</manifest>